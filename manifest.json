{
  "manifest_version": 2,

  "name": "Searcher",
  "description": "Smart Searching",
  "version": "0.1",
  "browser_action": {
    "default_icon": {
        "19":"media/search-70.png",
        "38":"media/search-70.png"
        },
    "default_popup": "search_box.html"
    },
  "content_scripts": [
    {
    "matches": ["http://*/*"],
    "js": ["lib/zepto.js","content_script.js"]
    }
  ]
}
